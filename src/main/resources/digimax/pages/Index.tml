<html t:type="layout" title="Index"
      t:sidebarTitle="FrameworkVersion"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">
<h1>${library.name}</h1>
<img src="${bannerImage}" alt="${library.name}"/>
    <!--<div id="wrapper">-->
    <div>
        <h3>${message:shelves}</h3>
        <ul t:type="loop" source="library.shelves" value="shelf" class="list double">
            <li><a t:type="pagelink" page="ShelfUi" context="shelf.id">${shelf.name}</a></li>
        </ul>
    </div>
</html>