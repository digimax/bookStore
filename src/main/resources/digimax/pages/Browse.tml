<html t:type="layout" title="Browse"
      t:sidebarTitle="FrameworkVersion"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
      xmlns:p="tapestry:parameter">

    <span t:type="loop" source="library.shelves" value="shelf">
        <span t:type="loop" source="shelf.books" value="book">
            <a t:type="pagelink" page="Shelves" context="book.id" title="${book.title}"><span t:type="png" fileName="${smallImageFileName}" altMessage="book.title"/></a>
            <!--<a t:type="pagelink" page="BookUi" context="book.id" title="${book.title}"><span t:type="png" fileName="${smallImageFileName}" altMessage="book.title"/></a>-->
        </span>
    </span>
</html>